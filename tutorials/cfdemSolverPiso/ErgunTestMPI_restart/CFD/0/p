FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 1.0e5;

boundaryField
{

    wall
    {
        type            zeroGradient;
    }

    inlet
    {
        type            zeroGradient;
        //type            fixedValue;
        //value           uniform 100000;
    }
    outlet
    {
        //type            zeroGradient;

        type            fixedValue;
        value           uniform 1.0e5;
    }
}

