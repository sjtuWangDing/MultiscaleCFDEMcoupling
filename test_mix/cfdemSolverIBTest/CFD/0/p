FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 100000;

boundaryField
{
    inlet      
    {
        type            zeroGradient;
    }

    wall      
    {
        type            zeroGradient;
    }

    outlet
    {
        type            zeroGradient;//fixedValue;
        //value           uniform 100000;
    }

    frontAndBack    
    {
        type            empty;
    }
}

